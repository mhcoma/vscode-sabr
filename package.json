{
    "name": "vscode-sabr",
    "displayName": "VSCode Sabr",
    "description": "Sabr Programming Language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "sabr",
            "aliases": ["Sabr", "sabr"],
            "extensions": [".sabrc"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "sabr",
            "scopeName": "source.sabr",
            "path": "./syntaxes/sabr.tmLanguage.json"
        }]
    }
}