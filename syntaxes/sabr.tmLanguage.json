{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Sabr",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#preproc"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.sabr",
				"match": "\\b(end|if|else|loop|while|break|continue|switch|case|pass|func|macro|defer|return|struct|member)\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.sabr",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.sabr",
					"match": "\\\\."
				}
			]
		},
		"preproc": {
			"name": "meta.preprocessor.sabr",
			"begin": "#",
			"end": "",
			"patterns": [
				{
					"name": "meta.preprocessor",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.sabr"
}